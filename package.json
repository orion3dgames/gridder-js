{
  "name": "gridder-js",
  "version": "0.0.1",
  "description": "A JS plugin that displays a thumbnail grid expanding preview similar to the effect seen on Google Images.",
  "keywords": [
    "js",
    "plugin",
    "thumbnail",
    "expanding",
    "grid"
  ],
  "source": "src/gridder-js.js",
  "main": "dist/gridder-js.js",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build && yarn run css",
    "css": "yarn sass src/:dist/ --style compressed",
    "watch-css": "yarn sass src/:dist/ --watch --style compressed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oriongunning/gridder.git"
  },
  "bugs": {
    "url": "https://github.com/oriongunning/gridder/issues"
  },
  "author": {
    "name": "Orion Gunning"
  },
  "license": "MIT",
  "devDependencies": {
    "parcel": "latest",
    "sass": "^1.33.0"
  }
}
